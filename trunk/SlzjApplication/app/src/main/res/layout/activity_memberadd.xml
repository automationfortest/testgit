<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_ef"
    android:orientation="vertical">

    <slzjandroid.slzjapplication.customView.NavigationView
        android:id="@+id/nav_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <GridView
        android:id="@+id/gv_member_add_choose_project_team"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/gray_ef"
        android:gravity="center"
        android:horizontalSpacing="@dimen/padding_1"
        android:numColumns="3"
        android:verticalSpacing="@dimen/padding_1"></GridView>
</LinearLayout>