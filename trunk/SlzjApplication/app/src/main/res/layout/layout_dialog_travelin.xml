<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_ef"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:gravity="center"
        android:paddingBottom="@dimen/padding_20"
        android:paddingTop="@dimen/padding_20"
        android:text="您存在未完成订单"
        android:textColor="@color/btn_black_font"
        android:textSize="@dimen/font_20_size" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/padding_1"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btn_dialog_travelin_left"
            android:layout_width="0dp"
            android:layout_height="@dimen/btn_height_50"
            android:layout_weight="1"
            android:background="@color/white"
            android:text="进入查看"
            android:textColor="@color/title_bg" />

        <Button
            android:id="@+id/btn_dialog_travelin_right"
            android:layout_width="0dp"
            android:layout_height="@dimen/btn_height_50"
            android:layout_weight="1"
            android:background="@color/white"
            android:text="再叫一单"
            android:textColor="@color/title_bg" />
    </LinearLayout>

</LinearLayout>